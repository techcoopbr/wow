-- Name: users_id_seq; Type: SEQUENCE SET; Schema: public; Owner: techcoop
--

SELECT pg_catalog.setval('public.users_id_seq', 42, true);


--
-- Name: ar_internal_metadata_pkey; Type: CONSTRAINT; Schema: public; Owner: techcoop
--

ALTER TABLE ONLY public.ar_internal_metadata
    ADD CONSTRAINT ar_internal_metadata_pkey PRIMARY KEY (key);


--
-- Name: blogs_pkey; Type: CONSTRAINT; Schema: public; Owner: techcoop
--

ALTER TABLE ONLY public.blogs
    ADD CONSTRAINT blogs_pkey PRIMARY KEY (id);


--
-- Name: ckeditor_assets_pkey; Type: CONSTRAINT; Schema: public; Owner: techcoop
--

ALTER TABLE ONLY public.ckeditor_assets
    ADD CONSTRAINT ckeditor_assets_pkey PRIMARY KEY (id);


--
-- Name: creators_pkey; Type: CONSTRAINT; Schema: public; Owner: techcoop
--

ALTER TABLE ONLY public.creators
    ADD CONSTRAINT creators_pkey PRIMARY KEY (id);


--
-- Name: friendly_id_slugs_pkey; Type: CONSTRAINT; Schema: public; Owner: techcoop
--

ALTER TABLE ONLY public.friendly_id_slugs
    ADD CONSTRAINT friendly_id_slugs_pkey PRIMARY KEY (id);


--
-- Name: game_creators_pkey; Type: CONSTRAINT; Schema: public; Owner: techcoop
--

ALTER TABLE ONLY public.game_creators
    ADD CONSTRAINT game_creators_pkey PRIMARY KEY (id);


--
-- Name: games_pkey; Type: CONSTRAINT; Schema: public; Owner: techcoop
--

ALTER TABLE ONLY public.games
    ADD CONSTRAINT games_pkey PRIMARY KEY (id);


--
-- Name: schema_migrations_pkey; Type: CONSTRAINT; Schema: public; Owner: techcoop
--

ALTER TABLE ONLY public.schema_migrations
    ADD CONSTRAINT schema_migrations_pkey PRIMARY KEY (version);


--
-- Name: users_pkey; Type: CONSTRAINT; Schema: public; Owner: techcoop
--

ALTER TABLE ONLY public.users
    ADD CONSTRAINT users_pkey PRIMARY KEY (id);


--
-- Name: index_blogs_on_creator_id; Type: INDEX; Schema: public; Owner: techcoop
--

CREATE INDEX index_blogs_on_creator_id ON public.blogs USING btree (creator_id);


--
-- Name: index_blogs_on_slug; Type: INDEX; Schema: public; Owner: techcoop
--

CREATE UNIQUE INDEX index_blogs_on_slug ON public.blogs USING btree (slug);


--
-- Name: index_ckeditor_assets_on_type; Type: INDEX; Schema: public; Owner: techcoop
--

CREATE INDEX index_ckeditor_assets_on_type ON public.ckeditor_assets USING btree (type);


--
-- Name: index_creators_on_user_id; Type: INDEX; Schema: public; Owner: techcoop
--

CREATE INDEX index_creators_on_user_id ON public.creators USING btree (user_id);


--
-- Name: index_friendly_id_slugs_on_slug_and_sluggable_type; Type: INDEX; Schema: public; Owner: techcoop
--

CREATE INDEX index_friendly_id_slugs_on_slug_and_sluggable_type ON public.friendly_id_slugs USING btree (slug, sluggable_type);


--
-- Name: index_friendly_id_slugs_on_slug_and_sluggable_type_and_scope; Type: INDEX; Schema: public; Owner: techcoop
--

CREATE UNIQUE INDEX index_friendly_id_slugs_on_slug_and_sluggable_type_and_scope ON public.friendly_id_slugs USING btree (slug, sluggable_type, scope);


--
-- Name: index_friendly_id_slugs_on_sluggable_type_and_sluggable_id; Type: INDEX; Schema: public; Owner: techcoop
--

CREATE INDEX index_friendly_id_slugs_on_sluggable_type_and_sluggable_id ON public.friendly_id_slugs USING btree (sluggable_type, sluggable_id);


--
-- Name: index_game_creators_on_creator_id; Type: INDEX; Schema: public; Owner: techcoop
--

CREATE INDEX index_game_creators_on_creator_id ON public.game_creators USING btree (creator_id);


--
-- Name: index_game_creators_on_game_id; Type: INDEX; Schema: public; Owner: techcoop
